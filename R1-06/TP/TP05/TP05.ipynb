    "# TP05 Suites Aliquotes\n",
    "---"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "def partiealiquote(n):\n",
    "    L = []\n",
    "    for i in range(1, n-1):\n",
    "        if n % i == 0:\n",
    "            L.append(i)\n",
    "    return L\n",
    "\n",
    "\n",
    "def suite_aliquote2(n):\n",
    "    L = partiealiquote(n)\n",
    "    return (prod(L)-1)/(prod(L)-1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [
